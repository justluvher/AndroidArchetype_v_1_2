<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <!-- state : disable/not pressed (disable)-->
	<item android:state_enabled="false" android:state_pressed="false" android:drawable="@drawable/common_rect_normal" />
	
	<!-- state : enable/not pressed (normal)-->
	<item android:state_enabled="true"  android:state_pressed="false" android:drawable="@drawable/common_rect_normal" />
	
	<!-- state : enable/pressed (pressed) -->
	<item android:state_enabled="true"  android:state_pressed="true"  android:drawable="@drawable/pressed_member_item" />
	
</selector>
